@model Inmobiliaria_.Net_Core.Models.Inmueble

<h2>Detalles del Inmueble</h2>

<dl class="row">
    <dt class="col-sm-2">Código</dt>
    <dd class="col-sm-10">@Model.IdInmueble</dd>

    <dt class="col-sm-2">Propietario</dt>
    <dd class="col-sm-10">@Model.Propietario?.Nombre @Model.Propietario?.Apellido</dd>

    <dt class="col-sm-2">Dirección</dt>
    <dd class="col-sm-10">@Model.Direccion</dd>

    <dt class="col-sm-2">Tipo</dt>
    <dd class="col-sm-10">@Model.Tipo</dd>

    <dt class="col-sm-2">Estado</dt>
    <dd class="col-sm-10">@Model.Estado</dd>
</dl>

<a class="btn btn-primary" asp-action="Index">Volver a la Lista</a>