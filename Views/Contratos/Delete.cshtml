@model Inmobiliaria_.Net_Core.Models.Contrato

<h2>Eliminar Contrato</h2>

<p>¿Está seguro de que desea eliminar este contrato?</p>

<dl class="row">
    <dt class="col-sm-2">Código</dt>
    <dd class="col-sm-10">@Model.IdContrato</dd>

    <dt class="col-sm-2">Inquilino</dt>
    <dd class="col-sm-10">@Model.IdInquilino</dd>

    <dt class="col-sm-2">Inmueble</dt>
    <dd class="col-sm-10">@Model.IdInmueble</dd>

    <dt class="col-sm-2">Monto Mensual</dt>
    <dd class="col-sm-10">@Model.MontoMensual</dd>

    <dt class="col-sm-2">Fecha Inicio</dt>
    <dd class="col-sm-10">@Model.FechaInicio.ToString("yyyy-MM-dd")</dd>

    <dt class="col-sm-2">Fecha Fin</dt>
    <dd class="col-sm-10">@Model.FechaFin.ToString("yyyy-MM-dd")</dd>
</dl>

<form asp-action="Delete" method="post">
    <input type="hidden" name="IdContrato" value="@Model.IdContrato" />
    <button type="submit" class="btn btn-danger">Eliminar</button>
    <a class="btn btn-secondary" asp-action="Index">Cancelar</a>
</form>